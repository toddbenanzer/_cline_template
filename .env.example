# Multi-Agent Auto-Researcher Environment Variables

# OpenRouter API Key (get from: https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google Custom Search API Key (get from: https://console.developers.google.com/)
GOOGLE_API_KEY=your_google_api_key_here

# Google Custom Search Engine ID (get from: https://cse.google.com/)
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Debug mode for detailed error output
DEBUG_MODE=False
